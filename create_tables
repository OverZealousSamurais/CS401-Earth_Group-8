USE `earth`;

CREATE TABLE `cards` (
	`id` INT(3) NOT NULL AUTO_INCREMENT,
    `plant_cost` INT(2) NOT NULL DEFAULT 0,
    `victory_points` INT(2) NOT NULL DEFAULT 0,
    `card_type` VARCHAR(16) NOT NULL DEFAULT '',
    `habitat_elements` VARCHAR(32) NOT NULL DEFAULT '',
    `card_name` VARCHAR(32) NOT NULL DEFAULT '',
    `scientific_name` VARCHAR(32) NOT NULL DEFAULT '',
    `growth_space` INT(1) NOT NULL DEFAULT 0,
    `max_growth_number` INT(2) NOT NULL DEFAULT 0,
    `canopy_completion_vp` INT(1) NOT NULL DEFAULT 0,
    `sprout_spaces` INT(1) NOT NULL DEFAULT 0,
    `ability_colors` VARCHAR(16) NOT NULL DEFAULT '',
    `first_ability_effect` VARCHAR(128) NOT NULL DEFAULT '',
    `second_ability_effect` VARCHAR(128) NOT NULL DEFAULT '',
    `objective` VARCHAR(128) NOT NULL DEFAULT '',
    `percentage_of_cards` FLOAT NOT NULL,
    PRIMARY KEY (`id`)
);

